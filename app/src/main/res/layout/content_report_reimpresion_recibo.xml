<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/txvReportReimprimeonReciboTitle"
        style="@style/StyleTextPrimaryLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/appMarginBottom"
        android:text="@string/report_text_title_reimpresion_recibo" />

    <Button
        android:id="@+id/btnReportReimpresionReciboUltimoRecibo"
        android:drawableStart="@drawable/if_back_in_time"
        style="@style/StyleButtonStandar"
        android:layout_alignParentStart="true"
        android:layout_below="@id/txvReportReimprimeonReciboTitle"
        android:text="@string/report_text_button_ultimo_recibo" />

    <Button
        android:id="@+id/btnReportReimpresionReciboNumeroDeCargo"
        android:drawableStart="@drawable/if_list_c"
        style="@style/StyleButtonStandar"
        android:layout_alignParentStart="true"
        android:layout_below="@id/btnReportReimpresionReciboUltimoRecibo"
        android:text="@string/report_text_button_numero_de_cargo" />

    <Button
        android:id="@+id/btnTransactionScreenBack"
        style="@style/StyleButtonBack"
        android:layout_alignParentStart="true"
        android:layout_below="@id/btnReportReimpresionReciboNumeroDeCargo"
        android:text="@string/general_text_button_regresar" />

</RelativeLayout>