<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/AppStyleActivity"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <FrameLayout
        android:id="@+id/frlPgbHldTestComunication"
        style="@style/StyleProgressbarHolder"
        android:visibility="gone">

        <ProgressBar
            android:id="@+id/pgbTestComunication"
            style="@style/StyleProgressbar" />
    </FrameLayout>


    <!--Header de los modulos-->
    <include
        android:id="@+id/headerContent"
        layout="@layout/content_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/appMarginBottom" />

    <include
        android:id="@+id/bodyContentConfigurationTest"
        layout="@layout/content_configuracion_test_paso_prueba"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/headerContent"
        android:visibility="visible" />

</RelativeLayout>