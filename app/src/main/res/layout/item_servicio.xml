<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/item_servicio_texto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/StyleTextPrimaryLarge"
        android:text="Titulo Frase"
        android:layout_marginBottom="3dp"
        android:layout_marginTop="3dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/item_servicio_icino"
        android:layout_toEndOf="@+id/item_servicio_icino" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="4dp"
        android:layout_marginBottom="3dp"
        android:layout_marginTop="3dp"
        android:id="@+id/item_servicio_icino"
        android:cropToPadding="false"
        android:src="@drawable/if_edit"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/item_servicio_texto"
        android:maxHeight="35dp"
        android:maxWidth="74dp"
        android:adjustViewBounds="true" />



</RelativeLayout >