<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/item_servicio_texto"
        style="@style/StyleTextPrimarySmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/item_servicio_icino"
        android:layout_marginStart="70dp"
        android:text="Rejalo" />

    <ImageView
        android:id="@+id/item_servicio_icino"
        android:layout_width="29dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="22dp"
        android:layout_marginTop="21dp"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        android:maxHeight="35dp"
        android:maxWidth="74dp"
        android:src="@drawable/if_edit" />

    <TextView
        android:id="@+id/item_servicio_valor"
        style="@style/StyleTextPrimaryLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/item_servicio_texto"
        android:layout_marginTop="13dp"
        android:text="$30.000" />


</RelativeLayout >